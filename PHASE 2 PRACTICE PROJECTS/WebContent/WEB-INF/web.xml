    <servlet-name>SuccessServlet</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Product</servlet-name>
    <servlet-class>com.Product</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Product</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Database</servlet-name>
  <servlet-class>com.Database</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Database</servlet-name>
  <url-pattern>/data</url-pattern></servlet-mapping>
